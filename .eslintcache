urn (\n    <div >\n      <div style={{textAlign:\"center\"}}>\n        <h2>Filter through by category:</h2>\n        <input type=\"text\" \n        onChange={(e) => {setCardFilter(e.target.value)}}/>\n      </div>\n      <h1 id='grocery-list-title'></h1>\n      <h3 id='grocery-status'></h3>\n      {cardList}\n    </div>\n  )\n}\n\nexport default connect(mapStateToProps)(Cards);","/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Profile.js",["62"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/App.js",["63","64"],"import './App.css';\nimport Register from './Components/Register';\nimport Login from './Components/Login';\nimport Profile from './Components/Profile';\nimport Cards from './Components/Cards';\nimport {useState} from 'react';\nimport Dashboard from './Components/Dashboard'\nimport {Route, Switch, Redirect, Link} from 'react-router-dom';\nimport {Component} from 'react';\nimport axios from 'axios';\nimport Logout from './Components/Logout';\nimport NewCard from './Components/NewCard';\nimport Nav from './Components/Nav';\n\n\nclass App extends Component{\n  constructor(){\n    super()\n\n\n    this.state = {\n      isLoggedIn: false,\n      currentUser: {\n        firstname: '',\n        lastname: '',\n        email: '',\n      },\n      menuOpen: false\n    }\n\n    this.toggleMenu = this.toggleMenu.bind(this);\n    this.setCurrentUser = this.setCurrentUser.bind(this);\n    this.setLoggedIn = this.setLoggedIn.bind(this);\n  }\n\n  componentDidMount(){\n    axios.get(\"http://localhost:1234/auth/userInfo\")\n    .then(res => {\n      \n      if(!res.data.firstname) {\n        console.log('dont have first name');\n      } else {\n        this.setState({\n          currentUser: res.data,\n          isLoggedIn: true\n        })\n      }\n      \n    }) \n  }\n\n  setCurrentUser(user){\n    console.log(user)\n    this.setState({\n      currentUser: user\n    })\n  }\n\n  setLoggedIn(userIsLoggedIn){\n    this.setState({\n      isLoggedIn: userIsLoggedIn\n    })\n  }\n\n  toggleMenu() {\n    this.setState({menuOpen: !this.state.menuOpen});\n  }\n\n  \n  render(){\n    return(\n      <div className=\"App\">\n        \n\n        {this.state.isLoggedIn ? \n        <>\n        <svg viewBox=\"0 0 100 80\" width=\"40\" height=\"40\" onClick={this.toggleMenu}>\n          <rect width=\"100\" height=\"20\"></rect>\n          <rect y=\"30\" width=\"100\" height=\"20\"></rect>\n          <rect y=\"60\" width=\"100\" height=\"20\"></rect>\n        </svg>\n        <Nav isLoggedIn={this.state.isLoggedIn} menuOpen={this.state.menuOpen} toggleMenu={this.toggleMenu} />\n         </> \n          \n        : null}\n    \n        <Switch> \n          <Route exact path='/' >\n          {this.state.isLoggedIn ? <Redirect to=\"/Dashboard\"/> : <Register \n            setLoggedIn={this.setLoggedIn}\n            setCurrentUser={this.setCurrentUser}/>\n            }\n           \n          </Route>\n          <Route path='/Login'>\n            <Login\n            setLoggedIn={this.setLoggedIn}\n            setCurrentUser={this.setCurrentUser}\n            isLoggedIn={this.state.isLoggedIn}\n            />\n          </Route>\n          <Route path='/Profile'>\n            <Profile\n            currentUser={this.state.currentUser}\n            isLoggedIn={this.state.isLoggedIn}/>\n          </Route>\n          <Route path='/Cards'>\n            <Cards/>\n          </Route>\n          <Route path='/Dashboard'>\n            <Dashboard\n            currentUser={this.state.currentUser}\n            isLoggedIn={this.state.isLoggedIn}/>\n          </Route>\n          <Route path='/Logout'>\n            <Logout\n            setCurrentUser={this.setCurrentUser}\n            setLoggedIn={this.setLoggedIn}/>\n          </Route>\n          <Route path=\"/NewCard\">\n            <NewCard\n            currentUser={this.state.currentUser}\n            isLoggedIn={this.state.isLoggedIn}/>\n          </Route>\n        </Switch>\n        \n    \n      </div>\n    )\n  }\n\n}\n\nexport default App;",["65","66"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Logout.js",["67"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/NewCard.js",["68"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Nav.js",[],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/index.js",[],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Register.js",[],{"ruleId":"69","severity":1,"message":"70","line":1,"column":8,"nodeType":"71","messageId":"72","endLine":1,"endColumn":13},{"ruleId":"69","severity":1,"message":"73","line":2,"column":7,"nodeType":"71","messageId":"72","endLine":2,"endColumn":19},{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":29,"column":6,"nodeType":"80","endLine":29,"endColumn":8,"suggestions":"81"},{"ruleId":"82","severity":1,"message":"83","line":70,"column":7,"nodeType":"84","endLine":70,"endColumn":35},{"ruleId":"82","severity":1,"message":"83","line":71,"column":7,"nodeType":"84","endLine":71,"endColumn":31},{"ruleId":"78","severity":1,"message":"85","line":14,"column":6,"nodeType":"80","endLine":14,"endColumn":24,"suggestions":"86"},{"ruleId":"69","severity":1,"message":"87","line":6,"column":9,"nodeType":"71","messageId":"72","endLine":6,"endColumn":17},{"ruleId":"69","severity":1,"message":"88","line":8,"column":34,"nodeType":"71","messageId":"72","endLine":8,"endColumn":38},{"ruleId":"74","replacedBy":"89"},{"ruleId":"76","replacedBy":"90"},{"ruleId":"78","severity":1,"message":"79","line":19,"column":6,"nodeType":"80","endLine":19,"endColumn":8,"suggestions":"91"},{"ruleId":"78","severity":1,"message":"92","line":22,"column":6,"nodeType":"80","endLine":22,"endColumn":13,"suggestions":"93"},"no-unused-vars","'axios' is defined but never used.",