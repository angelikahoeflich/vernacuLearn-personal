 = {\n  cards: []\n}\n\nconst UPDATE_CARDS = 'UPDATE_CARDS'\n\nexport default function cardReducer(state, action) {\n  if (typeof state === 'undefined') {\n    state = {\n      cards: []\n    } \n  }\n  const {type, payload} = action;\n  switch(type) {\n    case UPDATE_CARDS:\n      console.log(\"The payload:\", payload)\n      return {\n        ...state,\n        cards: payload\n      }\n    default:\n      return state;\n  }\n \n}","/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/ducks/store.js",[],["60","61"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Dashboard.js",[],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Cards.js",["62","63","64"],"import {useState, useEffect, Fragment} from 'react';\nimport { connect } from 'react-redux';\nimport axios from 'axios';\n\nconst mapStateToProps = (state) => {\n  return {\n    cards: state.cards\n  }\n}\n\nconst Cards = (props) => {\n  const [cardFilter, setCardFilter] = useState('');\n\n  useEffect(() => {\n\n    axios.get(\"http://localhost:1234/api/cards\")\n      .then( (res) => {\n       \n        props.dispatch({\n          type: \"UPDATE_CARDS\",\n          payload: res.data\n        })\n      })\n      .catch( (err) => console.log(\"err:\", err) )\n\n    return () => {\n      console.log(\"tearing down...\");\n    };\n  }, []);\n\n  \n\n\n\n  const cardList = props.cards\n    .filter( (card) => {\n      if(cardFilter.length < 2){\n        return true\n      } else {\n        return card.category.includes(cardFilter)\n      }\n    })\n    .map( (card) => {\n    return (\n      <Fragment>\n      \n      <div className=\"grocery-list-container\">\n        <div className=\"red-line\"></div>\n        <div className=\"input-container\">\n          <h4>{card.word_or_phrase}</h4>\n          <button className='pencil-btn'>Pencil icon goes here</button>\n        </div>\n        <ul id='grocery-list'>\n          <li>Definition: {card.definition}</li>\n          <li>Part of speech: {card.part_of_speech}</li>\n          <li>Difficulty: {card.difficulty}</li>\n          <li>Category: {card.category}</li>\n        </ul>\n      </div>\n      </Fragment>\n    );\n  })\n  return (\n    <div >\n      <div style={{textAlign:\"center\"}}>\n        <h2>Filter through by category:</h2>\n        <input type=\"text\" \n        onChange={(e) => {setCardFilter(e.target.value)}}/>\n      </div>\n      <h1 id='grocery-list-title'></h1>\n      <h3 id='grocery-status'></h3>\n      {cardList}\n    </div>\n  )\n}\n\nexport default connect(mapStateToProps)(Cards);","/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Profile.js",["65"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/App.js",[],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Logout.js",["66"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/NewCard.js",["67"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Nav.js",[],["68","69"],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/index.js",[],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Register.js",[],"/Users/angelikahoeflich/Desktop/devmtn/week8/vernaculearn-personal/src/Components/Interests.js",[],{"ruleId":"70","severity":1,"message":"71","line":1,"column":8,"nodeType":"72","messageId":"73","endLine":1,"endColumn":13},{"ruleId":"70","severity":1,"message":"74","line":2,"column":7,"nodeType":"72","messageId":"73","endLine":2,"endColumn":19},{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":29,"column":6,"nodeType":"81","endLine":29,"endColumn":8,"suggestions":"82"},{"ruleId":"83","severity":1,"message":"84","line":70,"column":7,"nodeType":"85","endLine":70,"endColumn":35},{"ruleId":"83","severity":1,"message":"84","